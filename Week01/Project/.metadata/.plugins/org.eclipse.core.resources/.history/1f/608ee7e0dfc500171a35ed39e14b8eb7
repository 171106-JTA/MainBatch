package com.revature.model.account;

import com.revature.model.Constants;

public final class MinorUserAccount extends UserAccount {

	/**
	 * 
	 */
	private static final long serialVersionUID = -7563722009182178006L;
	private static double interest = Constants.MINOR_INT_RATE;
	private static int limit = Constants.MINOR_CRED_LIMIT;
	private static int penalty = Constants.MINOR_USER_PENALTY;
	
	public MinorUserAccount() {
		super(limit, interest, penalty);
	}

	@Override
	public String toString() {
		return "MinorUserAccount [limit=" + limit + ", penalty=" + penalty + ", status=" + status + ", interest="
				+ interest + ", permissions=" + permissions + "]";
	}
	
	
}
