package com.revature.model.account;

import java.util.Date;
import java.util.Map;
import java.util.Set;

import com.revature.model.Loan;
import com.revature.model.Properties;
import com.revature.model.Transaction;
import com.revature.model.request.AccountRequest;
import com.revature.model.request.CancelRequest;
import com.revature.model.request.LoanRequest;

public final class SeniorUserAccount extends AdultUserAccount {
	private static double interest = Properties.SENIOR_INT_RATE;
	private static int limit = Properties.SENIOR_CRED_LIMIT;
	private static int penalty = Properties.SENIOR_USER_PENALTY;
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 8157795139108118928L;

	public SeniorUserAccount() {
		super(limit, interest, penalty);
	}
	
	public SeniorUserAccount(int limit, int penalty, int status, double interest, double balance, Date start,
			long successiveMillis, int successiveDays, boolean balanceNonNeg, boolean stateChange, int days, int years,
			int ssn, Map<Integer, Transaction> transactions, Map<Integer, Loan> loans,
			Set<LoanRequest> approvedLoanRequests, Set<LoanRequest> rejectedLoanRequests,
			Set<AccountRequest> approvedAccountRequests, Set<AccountRequest> rejectedAccountRequests,
			Set<CancelRequest> approvedRollbackRequests, Set<CancelRequest> rejectedRollbackRequests) {

		super(limit, penalty, status, interest, balance, start, successiveMillis, successiveDays, balanceNonNeg,
				stateChange, days, years, ssn, transactions, loans, approvedLoanRequests, rejectedLoanRequests,
				approvedAccountRequests, rejectedAccountRequests, approvedRollbackRequests, rejectedRollbackRequests);
	}
	@Override
	public String toString() {
		return "SeniorUserAccount [limit=" + limit + ", penalty=" + penalty + ", status=" + status + ", interest="
				+ interest + ", permissions=" + permissions + "]";
	}
	
	
}
