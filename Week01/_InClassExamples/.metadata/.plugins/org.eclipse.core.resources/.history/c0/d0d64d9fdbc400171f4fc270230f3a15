package com.revature.exception;

import com.revature.model.Constants;

public class PendingAccountException extends BankingException {

	/**
	 * 
	 */
	private static final long serialVersionUID = -1953815963890862337L;

	public PendingAccountException(String user) {
		super(Constants.ACCT_PENDING + user);
	}
}
