package com.revature.exception;

import com.revature.model.Constants;

public class RejectedAccountException extends BankingException {

	/**
	 * 
	 */
	private static final long serialVersionUID = -3105793979253085620L;
	
	public RejectedAccountException(String user) {
		super(user + ": "+ Constants.ACCT_REJECTED + "\n" + Constants.ADMIN_PHONE);
	}

}
