package com.revature.exception;

import com.revature.model.Constants;

public class UsernameInSessionException extends BankingException {

	/**
	 * 
	 */
	private static final long serialVersionUID = -5205283482780522031L;
	
	public UsernameInSessionException(String user) {
		super(Constants.ERR_USER_ONLINE + user);
	}

}
