package com.revature.model.account;

public class BasicUserAccount extends AdultUserAccount {

	public BasicUserAccount() {
		super();
	}
	
	public BasicUserAccount(String user, String pass) {
		super(user, pass);
	}

	@Override
	public void setArgs(Object... args) {
		setUser((String) args[0]);
		setPass((String) args[1]);
		setBalance((double) args[2]);
	} 
}
