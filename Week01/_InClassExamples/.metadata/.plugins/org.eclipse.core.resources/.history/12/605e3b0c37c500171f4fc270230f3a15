package com.revature.model.account;

public class MinorUserAccount extends UserAccount {

	/**
	 * 
	 */
	private static final long serialVersionUID = -7563722009182178006L;

	public MinorUserAccount() {
		super();
	}
	
	public MinorUserAccount(String user, String pass) {
		super(user, pass);
	}

	@Override
	public void setArgs(Object... args) {
		setUser((String) args[0]);
		setPass((String) args[1]);
		setBalance((double) args[2]);
	}
}
