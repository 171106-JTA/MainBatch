import java.util.*;

public class CompPojo implements Comparable<CompPojo> {
	private int id;
	private String name;
	private int age;	

	public CompPojo(int id, String name, int age) {
		super();
		this.id = id;
		this.name = name;
		this.age = age;
	}
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	
	@Override
	public String toString() {
		return "CompPojo [id=" + id + ", name=" + name + ", age=" + age + "]";
	}
	
	public void printAlternativeComp(List<CompPojo> list) {
		
		
	}

	@Override
	public int compareTo(CompPojo o) {
		if(this.id > o.id)
			return 1;
		if(this.id < o.id)
			return -1;
		return 0;
	}
	
}
